                <label_angle>73</label_angle>
            </display>
        </aux>
        <aux name="Length">
            <eqn>((((.75*X_sect_Area)^2)/lambda)^(1/3)) {from Equation 4 in the handout}</eqn>
            <display x="325" y="241" color="blue">
                <label_side>left</label_side>
                <label_angle>200</label_angle>
                <scale min="-325300" max="2047000" />
                <format precision="0" />
            </display>
        </aux>
        <aux name="initial_length_km">
