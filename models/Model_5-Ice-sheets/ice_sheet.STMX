            <eqn>IF Length&gt;snow_line_position THEN Length-snow_line_position ELSE 0</eqn>
            <display x="367" y="98" color="blue">
                <label_side>top</label_side>
                <label_angle>115</label_angle>
                <format precision="0" />
            </display>
        </aux>
        <stock name="X_sect_Area">
            <eqn>4/3*SQRT(lambda)*((initial_length_km*1e3)^(1.5))  { the initial cross-sectional area of the glacier, from equation 2 in the handout; this area grows/shrinks with time }</eqn>
            <inflow>area_change</inflow>
            <non_negative />
            <display x="429" y="214" color="blue">
                <label_side>right</label_side>
            </display>
        </stock>
        <flow name="area_change">
            <eqn>seed_area*ablation_rate + Accum_length*accumulation_rate - Ablation_length*ablation_rate {rate of change of area; equation 5 in handout; note that the first term is only there to deal with the case that the length is 0 AND the grounding line is larger than 0}</eqn>
